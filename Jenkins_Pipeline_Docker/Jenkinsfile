pipeline {
	agent any
	tools {
		node js "MyNode"
	}
	stages {
		stage("Check Node Version"){
			steps{
				sh "node --version"
			}
		}
		stage("Install Dependencies"){
			steps{
				sh "npm --version"
				sh "npm install"
			}
		}
		stage("Test"){
			steps{
				sh "node app.js"
			}
		}
		stage("Release"){
			steps{
				echo "Releasing this version"
			}
		}
	}
}